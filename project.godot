; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="YarnSpinner-GodotSharp"
run/main_scene="res://addons/yarnspinner_godot/demo/demo_scene.tscn"
config/features=PackedStringArray("4.1", "C#", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://addons/yarnspinner_godot/icons/YarnProjectIcon.png"
boot_splash/fullsize=false
config/icon="res://addons/yarnspinner_godot/icons/YarnProjectIcon.png"

[debug]

file_logging/enable_file_logging=true
file_logging/max_log_files=10

[dotnet]

project/assembly_name="YarnSpinner-GodotSharp"

[editor_plugins]

enabled=PackedStringArray("res://addons/csv-data-importer/plugin.cfg", "res://addons/yarnspinner_godot/plugin.cfg")

[internationalization]

locale/translation_remaps={}
locale/locale_filter_mode=0

[rendering]

renderer/rendering_method="mobile"

[yarn_spinner]

base_locale="en"
translations_directory="$(SourceDir)/translations/$(SourceFileName).csv"
