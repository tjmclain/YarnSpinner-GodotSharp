; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="YarnSpinner-GodotSharp"
config/features=PackedStringArray("4.1", "C#", "Mobile")
config/icon="res://icon.svg"

[dotnet]

project/assembly_name="YarnSpinner-Godot"

[internationalization]

locale/translation_remaps={}
locale/translations=PackedStringArray("res://translations/yarn_program.en.translation", "res://translations/yarn_program.fr.translation", "res://translations/yarn_program.jp.translation")
locale/locale_filter_mode=0

[rendering]

renderer/rendering_method="mobile"
